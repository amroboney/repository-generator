{
    "name": "amroboney/repository-generator",
    "description": "A feature-rich Laravel package to simplify and streamline the process of generating repository files using the repository pattern, promoting clean architecture and maintainable code.\n",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Amro Abuelyaman",
            "email": "amroboney@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AmroBoney\\RepositoryGenerator\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmroBoney\\RepositoryGenerator\\RepositoryGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
